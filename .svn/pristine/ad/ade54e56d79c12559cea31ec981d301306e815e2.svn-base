.examRight{
    height: 320px;
    width: 25%;
    margin-left: 5%;
    margin-bottom: 20px
}
.examLeft{
    height: 500px;
    width: 65%;
    margin-top: 30px;
    background-color: #fff;
    margin-bottom: 20px;
    position: relative
}
.examLeft>p{
    line-height: 30px;
}
.parentTitle{
    width: 100%;
    height: auto;
    padding: 10px;
    margin-bottom: 10px;
    background: rgba(54,113,207,.1);
}
.learnProper{
    font-size: 16px;
    color: #2e6da4;
}
.block{
    margin-top: 5px;
    text-indent: 1em;
    font-size: 14px!important;
    display: block;
}
.examRightPlate{
    padding: 5px 10px;
    min-height: 40px;width: 100%;background-color: #fff;text-align: center;margin-bottom: 10px;
    font-size: 14px;line-height: 40px;
}
.quitBtn{
    width: 100%;
    height: 100%;
    border: none!important;
    background-color: white
}

/*改动样式*/
.examTop{
    position: fixed;
    width:1200px;
    height:60px;
    top: 0;
    margin: auto;
    background-color: #fff;
}
.examTop li {
    float: left;
    width: 140px;
    height: 100%;
    line-height: 60px;
    text-align: center;
}
.quitBtn span:first-child{
    display: inline-block;
    font-size: 18px;
    margin-right: 10px;
}
.examNav{
    width:100%;
    height:80px;
    background-color: #959597;
    line-height: 80px;
    text-align: center;
    color: #fff;
    border:1px solid transparent;
}
.examNav>div{
    display: inline-block;
}
.examNav div:first-child{
    font-size: 22px;
    font-weight: 800;
}
