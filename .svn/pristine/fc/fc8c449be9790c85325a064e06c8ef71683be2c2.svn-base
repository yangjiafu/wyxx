.breadCrumb{
    margin: auto;
    width: 100%;
    padding: 0 10% 0 10%;
    height:60px;
    line-height: 60px;
    font-size: 14px;
    color: #9d9d9d;
}
.courseInfoSub{
    width: 80%;
    min-height: 210px;
    margin: auto;
    padding: 20px;
    background-color: #fff;
}
.courseContent{
    height: 170px;
    width: 60%;
    padding-left: 20px;
    float: left;
    display: inline-block;
    /*margin-left: 5%;*/
    /*border:1px solid red;*/
    /*background-color: #e3e3e3;*/
}
.courseContent>p{
    height: 40px;
    font-weight: normal;
    font-size: 14px;
    color: #9d9d9d;
}
.bg-title{
    padding: 2px 8px;
    color: #fff;
    background-color: #7cab7d;
}
.marginRight{
    margin-right: 15px;
}
.myBtnCourseItem{
    padding: 4px 8px;
    border-radius: 5px;
    font-size: 14px;
    background-color: #fff;
    color: #2166d5;
    margin-right: 10px;
    border:1px solid #2166d5;
}
.btnExamCourseItem{
    color:#c0c0c0;
    border:1px solid #c0c0c0;
}
.courseList{
    display: block;
    width: 80%;
    margin:20px auto 20px auto;
    background-color: #fff;
    height:60px;
}
.courseList ul li{
    font-size: 16px;
    line-height: 58px;
}
.activeBottom{
    color: #3671ce;
    box-sizing: border-box;
    border-bottom: 2px solid #3671ce;
}
.coursePlate{
    width: 80%;
    margin: auto;
    height:auto;
    background-color: #fff;
    padding: 16px;
}
#divBarLeft{

}

/*时间轴*/
.courseInfoSubMiddle{
    height: 100%;
    width: 125px;
}
.courseInfoSubTime{
    background: url("../img/icon/kuan.png") no-repeat;
    background-size: 100%;
    width: 90px;
    height: 100px;
    font-size: 26px;
    border-bottom:1px solid #D3D3D3;
    text-align: center;
    margin:25% 0 20% 20%;
}
.courseInfoSubMiddle span{
    margin-left: 20%;
    color:#BDBDBD;
}
.courseContent h3{
    margin: 20px 0;
}
.learnBtn{
    width: 80px;
    margin-right: 20px;
    background-color: #D9D9D9;
}


